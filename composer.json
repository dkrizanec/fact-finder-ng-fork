{
    "name": "dkrizanec/fact-finder-ng-fork",
    "type": "library",
    "description": "FactFinderNg module fork",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "spryker/elastica": "^4.0.0 || ^5.0.0 || ^6.0.0",
        "spryker/guzzle": "^2.0.0",
        "spryker/kernel": "^3.34.0",
        "spryker/locale": "^3.2.0 || ^4.0.0",
        "spryker/price-product-storage": "^4.0.0",
        "spryker/product-image-storage": "^1.10.0",
        "spryker/product-storage": "^1.13.0",
        "spryker/search": "^8.8.0",
        "spryker/store": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "spryker/catalog": "*",
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/sales": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\": "src/SprykerEco/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerEcoTest\\": "tests/SprykerEcoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
